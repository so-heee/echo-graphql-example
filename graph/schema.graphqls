# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type CategoryLarge {
  categoryLargeId: ID!
  categoryName: String!
}

type Query {
  categoryes: [CategoryLarge!]!
}

input NewCategoryLarge{
  categoryName: String!
}

type Mutation {
  createCategoryLarge(input: NewCategoryLarge!): CategoryLarge!
}