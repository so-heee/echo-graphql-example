# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type PageInfo {
  startCursor: Cursor
  endCursor: Cursor
  hasPreviousPage: Boolean!
  hasNextPage: Boolean!
}

interface Connection {
  edges: [Edge]
  nodes: [Node]
  pageInfo: PageInfo!
  totalCount: Int!
}

interface Edge {
  cursor: Cursor!
  node: Node!
}

interface Node {
  id: ID!
}
